{
  "name": "chiroport",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "dev:robust": "node scripts/dev-start.js",
    "dev:clean": "npm run clean && npm run dev",
    "dev:safe": "npm run cache:check && npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check": "npm run type-check && npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules/.cache && npm cache clean --force",
    "clean:full": "npm run clean && rm -rf node_modules && npm install",
    "reset": "pkill -f 'next dev' || true && npm run clean && npm run build",
    "cache:check": "node scripts/cache-monitor.js check",
    "cache:clean": "node scripts/cache-monitor.js clean",
    "cache:monitor": "node scripts/cache-monitor.js monitor",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:types": "supabase gen types typescript --local --schema public > src/lib/supabase/database.types.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.89.0",
    "axios": "^1.13.2",
    "framer-motion": "^12.15.0",
    "libphonenumber-js": "^1.12.8",
    "next": "^15.5.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-input-mask": "^2.0.4",
    "react-phone-number-input": "^3.4.12",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.14",
    "eslint": "^9",
    "eslint-config-next": "^15.5.9",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
